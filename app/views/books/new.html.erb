<h1>Books#new</h1>
<p>Find me in app/views/books/new.html.erb</p>
<%= form_with model:@book,url:books_path, local:true do |f| %>
    <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>
    <div>
        <%= f.label :body %>
        <%= f.text_area :body %>
    </div>
    <div>
        <%= f.submit '保存' %>
    </div>
    
<% end %>